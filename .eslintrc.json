{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "semi": "off"
  }
}
